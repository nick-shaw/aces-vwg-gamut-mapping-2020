  lim_x 0.6439501953
  addUserKnob {7 lim_y R -5 2}
  lim_y 0.8228656006
  addUserKnob {7 lim_z R -5 2}
  lim_z 0.7155163574
  addUserKnob {26 ""}
  addUserKnob {6 invert +STARTLINE}
  invert {{parent.direction}}
